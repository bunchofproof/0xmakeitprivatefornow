{
  "name": "zk-discord-verifier-web",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "validate-env": "dotenv -- tsx scripts/validate-env.ts",
    "dev": "next dev",
    "dev:https": "npm run validate-env && node server-https.js",
    "dev:public": "npm run validate-env && concurrently \"node server-https.js\" \"npm run tunnel\"",
    "dev:phone": "node start-public-dev.js",
    "tunnel": "ngrok http --url=https://192.168.1.8:3000 3000",
    "dev:next": "next dev",
    "build": "npm run validate-env && next build",
    "start": "npm run validate-env && next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@prisma/client": "^6.17.1",
    "@types/uuid": "^11.0.0",
    "@zkpassport/sdk": "^0.11.0",
    "concurrently": "^9.2.1",
    "critters": "^0.0.25",
    "diacritics": "^1.3.0",
    "ioredis": "^5.8.2",
    "next": "15.1.0",
    "prisma": "^6.17.1",
    "rate-limiter-flexible": "^8.1.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-qr-code": "^2.0.15",
    "selfsigned": "^3.0.1",
    "tailwindcss": "^3.4.1",
    "uuid": "^13.0.0",
    "viem": "^2.27.0",
    "zk-discord-verifier-shared": "file:../shared",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "dotenv-cli": "^10.0.0",
    "eslint": "^9.38.0",
    "eslint-config-next": "^16.0.1",
    "postcss": "^8",
    "tsx": "^4.20.6",
    "typescript": "^5"
  }
}
